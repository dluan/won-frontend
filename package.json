{
  "name": "skeleton",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "generate": "yarn plop --plopfile ./generators/plopfile.js"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@stripe/react-stripe-js": "^1.5.0",
    "@stripe/stripe-js": "^1.19.0",
    "@styled-icons/boxicons-regular": "^10.38.0",
    "@styled-icons/boxicons-solid": "^10.38.0",
    "@styled-icons/fa-brands": "^10.38.0",
    "@types/lodash.xor": "^4.5.6",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.0",
    "joi": "^17.4.2",
    "lodash.xor": "^4.5.0",
    "next": "11.1.2",
    "next-auth": "^3.29.0",
    "next-pwa": "^5.3.1",
    "next-seo": "^4.28.1",
    "nextjs-progressbar": "^0.0.11",
    "node-jose-tools": "^1.6.28",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.1",
    "styled-icons": "^10.40.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@types/react-slick": "^0.23.5",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "apollo": "^2.33.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "cross-env": "^7.0.3",
    "cypress": "^8.5.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "jest": "^27.2.1",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "msw": "^0.35.0",
    "node-fetch": "2.6.1",
    "plop": "^2.7.4",
    "prettier": "2.3.2",
    "typescript": "^4.4.2"
  }
}
